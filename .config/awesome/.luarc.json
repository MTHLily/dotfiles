{
  "diagnostics": {
    "enable": true,
    "globals": ["awesome", "tag", "client", "root", "screen", "mouse"],
    "disable": ["lowercase-global"]
  },
  "runtime": {
    "version1": "Lua 5.4",
    "version": "LuaJIT",
    "pathStrict": true
  },
  "workspace": {
    "ignoreDir": ["tmp", "dotfiles", "bling"],
    "library": ["internal", "/usr/share/awesome/lib/", "./bling"]
  },
    "workspace.checkThirdParty": false
}
